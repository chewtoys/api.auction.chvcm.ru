{
  "name": "@alendo/api.auction.chvcm.ru",
  "version": "1.0.0",
  "description": "https://auction.chvcm.ru backend",
  "main": "dist/index.js",
  "bin": {
    "api.auction.chvcm.ru-migrate": "dist/bin/migrate.js",
    "api.auction.chvcm.ru-mkadmin": "dist/bin/mkadmin.js",
    "api.auction.chvcm.ru-test-email": "dist/bin/test-email.js",
    "api.auction.chvcm.ru-web": "dist/bin/web.js"
  },
  "scripts": {
    "build-src": "rimraf dist && tsc",
    "build-public": "rimraf build && nuxt build",
    "build-apidoc": "apidoc -i routes -i src -o apidoc",
    "postinstall": "yarn build-src && yarn build-public",
    "test": "rimraf dist && mocha",
    "test-cov": "rimraf dist && nyc mocha",
    "lint": "tslint -c tslint.json -e './node_modules/**/*' './**/*.ts'",
    "loc": "sloc global.ts index.ts bin db routes src types",
    "loc-test": "sloc test",
    "public-dev": "nuxt",
    "migrate-dev": "ts-node bin/migrate.ts",
    "migrate": "node dist/bin/migrate.js",
    "mkadmin-dev": "ts-node bin/mkadmin.ts",
    "mkadmin": "node dist/bin/mkadmin.js",
    "test-email": "node dist/bin/test-email.js",
    "test-email-dev": "ts-node bin/test-email.ts",
    "web": "node dist/bin/web.js",
    "web-dev": "ts-node bin/web.ts"
  },
  "apidoc": {
    "name": "Интернет-аукцион",
    "version": "1.0.0",
    "description": "Документация RESTful API",
    "url": "/api/v1",
    "template": {
      "forceLanguage": "ru"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/index.ts",
      "**/interfaces/**/*.ts",
      "test/**/*.ts",
      "apidoc",
      "bin",
      "build",
      "coverage",
      "dist",
      "docker",
      "public"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlendoStudio/api.auction.chvcm.ru.git"
  },
  "keywords": [
    "alendo",
    "nodejs",
    "typescript",
    "backend",
    "auction"
  ],
  "author": "Alendo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlendoStudio/api.auction.chvcm.ru/issues"
  },
  "homepage": "https://github.com/AlendoStudio/api.auction.chvcm.ru#readme",
  "dependencies": {
    "@alendo/express-req-validator": "https://github.com/AlendoStudio/node-express-req-validator.git#5ee4577e52ae490d44a08c534da1c40236c5d16c",
